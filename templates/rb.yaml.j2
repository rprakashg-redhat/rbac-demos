{% for item in customRoles.contributor.assignTo %}
---
apiVersion: rbac.authorization.k8s.io/v1
kind: RoleBinding
metadata:
    name: {{ item.claim }}-{{ customRoles.contributor.name }}
    namespace: {{ item.namespace }}
roleRef:
    kind: ClusterRole
    name: {{ customRoles.contributor.name }}
    apiGroup: rbac.authorization.k8s.io
subjects:
- apiGroup: rbac.authorization.k8s.io
  kind: Group
  name: {{ item.claim }}
  namespace: {{ item.namespace }}
{% endfor %}
